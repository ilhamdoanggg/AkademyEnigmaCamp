<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC
"-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>Archetype Created Web Application</display-name>

    <servlet>
      <servlet-name>HelloServlet</servlet-name>
      <servlet-class>com.enigma.Hello</servlet-class>
    </servlet>
    <servlet>
      <servlet-name>HaiServlet</servlet-name>
      <servlet-class>com.enigma.ApaKabar</servlet-class>
    </servlet>
    <servlet>
      <servlet-name>RobotServlet</servlet-name>
      <servlet-class>com.enigma.RobotServlet</servlet-class>
    </servlet>

    <servlet-mapping>
      <servlet-name>HelloServlet</servlet-name>
      <url-pattern>/hello.doang</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
      <servlet-name>HaiServlet</servlet-name>
      <url-pattern>/apakabar.doang</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
      <servlet-name>RobotServlet</servlet-name>
      <url-pattern>/robot.doang</url-pattern>
    </servlet-mapping>

</web-app>
