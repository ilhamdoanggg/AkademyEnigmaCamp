<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.enigma.model.Students" table="students">
    <id name="id" column ="Id_student" type="integer"></id>
    <property name="name" column="student_name" type="string"> </property>
    <property name="birthPlace" column="birth_place" type="string"> </property>
    <property name="birthDate" column="birth_date" type="date"> </property>
        <property name="gender" column="gender" type="string"/>
    </class>
</hibernate-mapping>