<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC
"-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_4.xsd"
         version="2.4">
  <display-name>Archetype Created Web Application</display-name>

    <servlet>
      <servlet-name>HelloServlet</servlet-name>
      <servlet-class>com.enigma.Hello</servlet-class>
    </servlet>
    <servlet>
      <servlet-name>HaiServlet</servlet-name>
      <servlet-class>com.enigma.ApaKabar</servlet-class>
    </servlet>
    <servlet>
      <servlet-name>RobotServlet</servlet-name>
      <servlet-class>com.enigma.RobotServlet</servlet-class>
    </servlet>

  <servlet>
    <servlet-name>StudentServlet</servlet-name>
    <servlet-class>com.enigma.StudentServlet</servlet-class>
  </servlet>

    <servlet-mapping>
      <servlet-name>HelloServlet</servlet-name>
      <url-pattern>/hello.doang</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
      <servlet-name>HaiServlet</servlet-name>
      <url-pattern>/apakabar.doang</url-pattern>
    </servlet-mapping>

  <servlet-mapping>
    <servlet-name>StudentServlet</servlet-name>
    <url-pattern>/student.doang</url-pattern>
  </servlet-mapping>

    <servlet-mapping>
      <servlet-name>RobotServlet</servlet-name>
      <url-pattern>/robot.doang</url-pattern>
    </servlet-mapping>

</web-app>
